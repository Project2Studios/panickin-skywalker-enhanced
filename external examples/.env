# Firebase Configuration for Mindra Analytics
# Get these values from your Firebase Console

# Client-side Firebase config (safe to expose)
VITE_FIREBASE_API_KEY=your-api-key-here
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=1:123456789:web:abcdef123456
VITE_FIREBASE_MEASUREMENT_ID=G-ABCDEF1234

# Server-side config
USE_FIREBASE=false
NODE_ENV=development

# Firebase Admin SDK (keep this secret!)
# Get this from Firebase Console > Project Settings > Service Accounts > Generate Key
FIREBASE_SERVICE_ACCOUNT_KEY='{"type":"service_account","project_id":"your-project",...}'

# Optional: Database settings
DATABASE_URL=postgresql://user:password@localhost:5432/mindra

# Optional: Analytics settings  
ANALYTICS_DEBUG=true
ANALYTICS_SESSION_TIMEOUT=30
